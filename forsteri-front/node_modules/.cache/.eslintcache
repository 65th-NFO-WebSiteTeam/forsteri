rue })} />\r\n          <br />\r\n          <input className=\"button-submit\" type=\"submit\" value=\"ログイン\" />\r\n        </form>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n","C:\\Users\\kaito\\Desktop\\Coding\\NF\\forsteri\\forsteri-front\\src\\auth\\pages\\Logout.js",["98"],[],"import React from 'react';\r\nimport { useCookies } from 'react-cookie';\r\nimport { useNavigate } from 'react-router-dom';\r\n\r\nconst Logout = ({ onLogout }) => {\r\n  const navigate = useNavigate();\r\n  const [cookies, removeCookie] = useCookies(['accesstoken', 'refreshtoken']);\r\n\r\n  const handleLogout = () => {\r\n    // アクセストークンとリフレッシュトークンのクッキーを削除\r\n    removeCookie('accesstoken', { path: '/' });\r\n    removeCookie('refreshtoken', { path: '/' });\r\n\r\n    // ログアウト後のリダイレクト先へ移動\r\n    navigate('/');\r\n\r\n    // ログアウト時にログイン状態を更新\r\n    onLogout();\r\n  };\r\n\r\n  return (\r\n    <div className='logout-wrapper'>\r\n      <button className='button-submit set-center' onClick={handleLogout}>ログアウト</button>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Logout;\r\n\r\n","C:\\Users\\kaito\\Desktop\\Coding\\NF\\forsteri\\forsteri-front\\src\\auth\\pages\\Signup.js",[],[],"C:\\Users\\kaito\\Desktop\\Coding\\NF\\forsteri\\forsteri-front\\src\\theme\\pages\\StaffThemeDetails.js",[],[],"C:\\Users\\kaito\\Desktop\\Coding\\NF\\forsteri\\forsteri-front\\src\\theme\\api\\voteTheme.js",[],[],"C:\\Users\\kaito\\Desktop\\Coding\\NF\\forsteri\\forsteri-front\\src\\theme\\components\\StaffThemeContent.js",[],[],"C:\\Users\\kaito\\Desktop\\Coding\\NF\\forsteri\\forsteri-front\\src\\theme\\pages\\StaffThemeTop.js",[],[],[],{"ruleId":"99","severity":1,"message":"100","line":11,"column":10,"nodeType":"101","messageId":"102","endLine":11,"endColumn":17},{"ruleId":"99","severity":1,"message":"103","line":12,"column":35,"nodeType":"101","messageId":"102","endLine":12,"endColumn":41},{"ruleId":"99","severity":1,"message":"100","line":7,"column":10,"nodeType":"101","m